public class NumerosImpares {
    public static void main(String[] args) {
        System.out.println("Números ímpares entre 1 e 100:");

        int i = 1;
        while (i <= 100) {
            if (i % 2 != 0) {
                System.out.println(i);
            }
            i++;
        }
    }
}
