public class SequenciaFibonacci {
    public static void main(String[] args) {
        int primeiro = 0;
        int segundo = 1;
        int contador = 0;

        while (contador < 10) { // Queremos os 10 primeiros números
            System.out.print(primeiro);

            if (contador < 9) {
                System.out.print(", "); // Coloca vírgula só entre os números
            }

            int proximo = primeiro + segundo; // Próximo número da sequência
            primeiro = segundo; // Atualiza o primeiro
            segundo = proximo; // Atualiza o segundo

            contador++; // Conta quantos números já imprimimos
        }
    }
}
